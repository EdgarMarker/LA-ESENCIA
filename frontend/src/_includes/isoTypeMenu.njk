<div id="isoType">
    {%for logo in logosEmpresa%}
    <a href="/">
        <img src="{{logo.isoType.media.url}}" alt="Iso tipo" />
    </a>
    {%endfor%}
</div>
<style>
    #isoType {
        position: fixed;
        padding: 5px;
        z-index: 50;
        left: 5px;
        top: 5px;
        width: 50px;
        height: 50px;
        border-radius: 50%;
        cursor: pointer;
        transform-origin: center;
        transition: transform 0.3s ease-in-out;
        transform: translateX(-100%);
        opacity: 0;
        background: var(--bg-primary);
        img, a {
            height: 100%;
            object-fit: cover;
        }
    }
    @media only screen and (max-width: 1024px) {
        #isoType {
            transform: translateX(0%) !important;
            opacity: 1;
        }
    }
</style>